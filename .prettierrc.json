{
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "endOfLine": "lf",
  "trailingComma": "all",
  "jsxSingleQuote": true,
  "importOrder": ["^#(.*)/", "^[./]"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy", "importAssertion"]
}
