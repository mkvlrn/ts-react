{
  "name": "ts-react",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn tsc && npx vite build",
    "check-types": "yarn tsc-files",
    "dev": "yarn vite",
    "format": "yarn prettier --write",
    "lint": "yarn eslint --fix",
    "prebuild": "yarn rimraf build",
    "prepare": "yarn husky install",
    "start": "node build",
    "test-staged": "cross-env NODE_OPTIONS=--experimental-vm-modules yarn jest --bail --findRelatedTests",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.3",
    "@jest/globals": "^29.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "emotion-normalize": "^11.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
