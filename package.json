{
  "name": "ts-react",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn tsc && npx vite build",
    "check-types": "yarn tsc-files",
    "dev": "yarn vite",
    "format": "yarn prettier --write",
    "lint": "yarn eslint --fix",
    "prebuild": "yarn rimraf build",
    "prepare": "yarn husky install",
    "start": "node build",
    "test": "yarn vitest --run",
    "test-cov": "rimraf coverage && yarn vitest run --coverage",
    "test-staged": "yarn vitest related --run"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest": "^0.0.57",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "typescript": "^5.0.3",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "emotion-normalize": "^11.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^0.30.1"
  }
}
